
#header div{
	width:100%;
	height:80px;
	line-height:80px;
	background:#292929;
	text-align:center;
	border-bottom:1px solid #383838;
}
#header img{
	width:130px;
	height:50px;
	vertical-align:middle;
	margin-right:26%;
}
#header a{
	text-decoration:none;
	color:#848484;
	font-size:14px;
	margin-left:60px;
}

#header a:hover{
	cursor:pointer;
	color:#BABABA;
}

#footer div{
	width:100%;
	height:142px;
	background:#292929;
	color:#848484;
	font-size:12px;
	text-align:center;
	border-top:1px solid #383838;
}
#footer img{
	width:24px;
	height:24px;
	margin:0 3px;
}
#footer div:before{
	content:"";
	display:table;
	padding:5px;
}
#footer div p{
	margin-top:28px;
}